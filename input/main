import std
import dog

main():
    print("Running Main")

    d = Dog()
    print(d.sound())

    for n in [10, 5]:
        i = n
        while i > 0:
            i = i - 1
            if i % 2 == 0:
                print("EVEN")
            else:
                print("ODD")
            end
            print(i)
        end
    end

    return 0
