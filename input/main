import std

class Animal():
    sound():
        return "none" 
end

class Dog(Animal):
    sound():
        return "bark"
end

main():
    print("Running Main")
    for n in range(10):
        i = n
        while i > 0:
            i = i - 1
            if i % 2 == 0:
                print("EVEN")
            else:
                print("ODD")
            end
            print(i)
        end
    end
    return 0
