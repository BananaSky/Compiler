import dog

main():
    print("Running Main")
    dogs = [Dog(), Dog()]
    for dog in dogs:
        print(dog.sound())

    for n in [10, 5]:
        i = n
        while i > 0:
            i = i - 1
            if i % 2 == 0:
                print("EVEN")
            else:
                print("ODD")
            print(i)
    return 0
