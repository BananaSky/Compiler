foo(x, y):
    return x * y

class Animal():
    __init__(set_name, set_sound):
        print("Creating Animal")
        self.name  = set_name
        self.sound = set_sound

class Dog(Animal):
    __init__(set_name, set_type):
        super().__init__(set_name, "bark")
        self.type = set_type

# Converts to int main()
main():
    print(foo(2, 3))
    return 0
